{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.5.3",
      "destination": "wwwroot/lib/bootstrap/dist/",
      "files": [ "css/bootstrap.min.css", "js/bootstrap.bundle.min.js" ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/dist/",
      "files": [ "jquery.min.js" ]
    },
    {
      "library": "popper.js@2.5.4",
      "destination": "wwwroot/lib/popper",
      "files": [ "esm/popper.min.js" ]
    },
    {
      "library": "AlertifyJS@1.13.1",
      "destination": "wwwroot/lib/alertify",
      "files": ["alertify.min.js","css/alertify.min.css"]
    }
      
  ]
}